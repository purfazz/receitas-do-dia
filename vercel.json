{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_GEMINI_API_KEY": "@gemini_api_key"
  },
  "github": {
    "enabled": true,
    "silent": true
  }
} 